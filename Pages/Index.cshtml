<!-- Carousel -->
<div id="main-slide" class="carousel slide" data-ride="carousel">

    <!-- Indicators -->
    <ol class="carousel-indicators visible-lg visible-md">
        <li data-target="#main-slide" data-slide-to="0" class="active"></li>
        <li data-target="#main-slide" data-slide-to="1"></li>
        <li data-target="#main-slide" data-slide-to="2"></li>
    </ol>
    <!--/ Indicators end-->
    <!-- Carousel inner -->
    <div class="carousel-inner">

        <div class="item active" style="background-image:url(https://bit.ly/2WPy9Ge);">
            <div class="slider-content">
                <div class="col-md-12 text-center">
                    <h2 class="slide-title animated4">LEND CAR</h2>
                    <h3 class="slide-sub-title animated5">The new way of having your car</h3>
                    <p>
                        <a href="/allCar" class="slider btn btn-primary">Choose your car</a>
                        <a href="/contact" class="slider btn btn-primary border">Contact Now</a>
                    </p>
                </div>
            </div>
        </div>
        <!--/ Carousel item 1 end -->

        <div class="item" style="background-image:url(https://bit.ly/2Wxond3)">
            <div class="slider-content text-left">
                <div class="col-md-12">
                    <h3 class="slide-title animated3">Your car will be safe</h3>
                    <h3 class="slide-sub-title animated3">Earn a lot of money</h3>
                    <p class="animated3">
                        <a href="~/Cars/addCar" class="slider btn btn-primary ">Add your car</a>
                    </p>
                </div>
            </div>
        </div>
        <!--/ Carousel item 2 end -->

        <div class="item" style="background-image:url(https://bit.ly/2Z0cOwp)">
            <div class="slider-content text-right">
                <div class="col-md-12">
                    <h3 class="slide-sub-title slide-title animated7">Choose your car</h3>
                    <p class="slider-description lead animated7">
                        Drive your next car for as long as you want for a fixed Daily price
                    </p>
                </div>
            </div>
        </div>
        <!--/ Carousel item 3 end -->

    </div><!-- Carousel inner end-->
    <!-- Controllers -->
    <a class="left carousel-control" href="#main-slide" data-slide="prev">
        <span><i class="fa fa-angle-left"></i></span>
    </a>
    <a class="right carousel-control" href="#main-slide" data-slide="next">
        <span><i class="fa fa-angle-right"></i></span>
    </a>
</div>
<!--/ Carousel end -->


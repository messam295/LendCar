<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <title>@ViewData["Title"] - LendCar</title>


    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <!-- Responsive styles-->
    <link rel="stylesheet" href="~/css/responsive.css">
    <!-- FontAwesome -->
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <!-- Animation -->
    <link rel="stylesheet" href="~/css/animate.css">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/css/owl.theme.default.min.css">
    <!-- Template styles-->
    <link href="~/css/style.css" rel="stylesheet" />
    <!-- Colorbox -->
    <link rel="stylesheet" href="~/css/colorbox.css">
    <!-- index page of all car -->
    <link href="~/css/HomeStyle.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
    <!-- font -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Lemon" />
    @RenderSection("Styles", false)
</head>
<body>
    <div class="body-inner">

        <!--/ settingBox satart -->
        <div class="settings-box">
            <div class="toggle-settings">
                <i class="fas fa-gear "></i>
            </div>
            <div class="carGo">
                <img src="https://bit.ly/2zeXUrC" />
            </div>
            <div class="settings-container">
                <div class="option-box">
                    <ul class="colors-list">
                        <li class="activ" data-color="#ffb600"></li>
                        <li data-color="cornflowerblue"></li>
                        <li data-color="tomato"></li>
                        <li data-color="forestgreen"></li>
                    </ul>
                    <h4>Colors</h4>
                </div>
            </div>
        </div>
        <!--/ settingBox end -->
        <!-- Header start -->
        <header id="header" class="header-one">
            <nav class="site-navigation navigation navdown">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="site-nav-inner">
                                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                                <div class="logo col-xs-12 col-md-2">
                                    <a href="Index">
                                        <img src="~/logo.png" style="height:56px" alt="">
                                    </a>
                                </div>
                                <div class="collapse navbar-collapse navbar-responsive-collapse">
                                    <ul class="nav navbar-nav">
                                        <li class="nav-item"><a href="~/Index">Home</a></li>
                                        <li class="nav-item"><a asp-page="AllCars" asp-area="Cars">All Cars</a></li>
                                        @if (User.Identity.IsAuthenticated)
                                        {
                                            <li> <partial name="_AddCarPartial" /></li>
                                        }
                                        <li class="nav-item"><a asp-page="HowUse">How to use</a></li>
                                        <li class="nav-item"><a asp-page="contact">Contact</a></li>
                                        <li class="nav-item"><a asp-page="about">About</a></li>
                                        @if (User.IsInRole("Admin"))
                                        {
                                            <li class="dropdown nav-item">
                                                <a class="dropdown-toggle" data-toggle="dropdown">Admin<i class="fa fa-angle-down"></i></a>
                                                <ul class="dropdown-menu" role="menu">
                                                    <li><a asp-area="Admin" asp-page="Admin">CRUD</a></li>
                                                    <li><a asp-area="Admin" asp-page="BookingMoney">Booking Money</a></li>
                                                    <li><a asp-area="Admin" asp-page="ClientHistory">Client History</a></li>
                                                    <li><a asp-area="Admin" asp-page="Users">Users</a></li>
                                                </ul>
                                            </li>
                                        }

                                    </ul>

                                    <ul class="nav navbar-nav navbar-right">
                                        <partial name="_LoginPartial" />
                                    </ul>

                                    <!--/ Nav ul end -->

                                </div>
                                <!--/ Collapse end -->

                            </div><!-- Site Navbar inner end -->

                        </div>
                        <!--/ Col end -->
                    </div>
                    <!--/ Row end -->
                    @*<div class="nav-search">
                        <span id="search"><i class="fa fa-search"></i></span>
                    </div>*@


                    @*<div class="search-block" style="display: none;">
                        <input type="text" class="form-control" placeholder="Type what you want and enter">
                        <span class="search-close">&times;</span>
                    </div>*@
                </div>
                <!--/ Container end -->

            </nav>
            <!--/ Navigation end -->
        </header>
        <!--/ Header end -->

        <div class="container-fluid  no-padding">
            @*<main role="main" class="pb-3">*@
            @RenderBody()

        </div>




        <footer id="footer" class="footer bg-overlay">

            <div class="footer-main">
                <div class="container">
                    <div class="row">
                        <div class="col-md-4 col-sm-12 footer-widget footer-about">
                            <h3 class="widget-title">About Us</h3>
                            <img class="footer-logo" src="images/footer-logo.png" alt="" />
                            <p>
                                LendCar is a car sharing marketplace where guests can book any car they want,
                                wherever they want it across All governorates of Egypt.
                            </p>
                            <div class="footer-social">
                                <ul>
                                    <li><a href="#"><i class="fab fa-facebook"></i></a></li>
                                    <li><a href="#"><i class="fab fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fab fa-instagram"></i></a></li>
                                    <li><a href="#"><i class="fab fa-github"></i></a></li>
                                </ul>
                            </div><!-- Footer social end -->
                        </div><!-- Col end -->

                        <div class="col-md-4 col-sm-12 footer-widget">
                            <h3 class="widget-title">Working Hours</h3>
                            <div class="working-hours">
                                We work 7 days a week, every day excluding major holidays. Contact us if you have an emergency, with our
                                Hotline and Contact form.
                                <br><br> Monday - Friday: <span class="text-right">10:00 - 16:00 </span>
                                <br> Saturday: <span class="text-right">12:00 - 15:00</span>
                                <br> Sunday and holidays: <span class="text-right">09:00 - 12:00</span>
                            </div>
                        </div><!-- Col end -->

                        <div class="col-md-4 col-sm-12 footer-widget">
                            <h3 class="widget-title">Services</h3>
                            <ul class="list-arrow">
                                <li><a asp-area="Cars" asp-page="AllCars">All cars</a></li>
                                <li><a asp-area="Cars" asp-page="AddCar">Add new car</a></li>
                                <li><a asp-page="HowUse">How does it work</a></li>
                                <li><a asp-page="contact">Contact us</a></li>
                            </ul>
                        </div><!-- Col end -->


                    </div><!-- Row end -->
                </div><!-- Container end -->
            </div><!-- Footer main end -->

            <div class="copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12 col-sm-6">
                            <div class="copyright-info">
                                <span>Copyright © 2020  <a href="">ITI MINIA</a></span>
                            </div>
                        </div>

                        <div class="col-xs-12 col-sm-6">
                            <div class="top-social text-right">
                                <ul class="unstyled">
                                    <li>
                                        <a title="Facebook" href="https://facebbok.com/themefisher.com">
                                            <span class="social-icon"><i class="fab fa-facebook"></i></span>
                                        </a>
                                        <a title="Twitter" href="https://twitter.com/themefisher.com">
                                            <span class="social-icon"><i class="fab fa-twitter"></i></span>
                                        </a>
                                        <a title="Instagram" href="https://instagram.com/themefisher.com">
                                            <span class="social-icon"><i class="fab fa-instagram"></i></span>
                                        </a>
                                        <a title="Linkdin" href="https://github.com/themefisher.com">
                                            <span class="social-icon"><i class="fab fa-linkedin"></i></span>
                                        </a>
                                    </li>
                                </ul>
                            </div>

                        </div>
                    </div><!-- Row end -->

                    <div id="back-to-top" data-spy="affix" data-offset-top="10" class="back-to-top affix">
                        @*<button class="btn btn-primary" title="Back to Top">
                            <i class="fa fa-angle-double-up"></i>
                        </button>*@
                        <span title="Back to Top" style="cursor:pointer;">
                            <img src="http://themes.potenzaglobalsolutions.com/html/cardealer/assets/images/car.png" alt="">
                        </span>
                    </div>

                </div><!-- Container end -->
            </div><!-- Copyright end -->

        </footer><!-- Footer end -->


        <div id="loader-wrapper">
            <div id="loader"></div>
            <div class="loader-section section-left"></div>
            <div class="loader-section section-right"></div>
        </div>

        <!-- Javascript Files
    ================================================== -->
        <!-- initialize jQuery Library -->
        <script src="~/js/jquery.js"></script>
        <!-- jQuery Validation Libraries -->
        <script src="~/js/jquery-validate/jquery.validate.min.js"></script>
        <script src="~/js/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
        <!-- Bootstrap jQuery -->
        <script src="~/js/bootstrap.min.js"></script>
        <!-- Owl Carousel -->
        <script src="~/js/owl.carousel.min.js"></script>
        <!-- Color box -->
        <script src="~/js/jquery.colorbox.js"></script>
        <!-- Isotope -->
        <script src="~/js/isotope.js"></script>
        <script src="~/js/ini.isotope.js"></script>
        <!-- wow library -->
        <script src="~/js/wow.min.js"></script>
        <!-- Template custom -->
        <script src="~/js/custom.js"></script>
        <!--handroom plugin -->
        <script src="https://unpkg.com/headroom.js"></script>
        @RenderSection("Scripts", false)
        <script>

            $(document).ready(function () {
                debugger;
                var navItems = document.getElementsByClassName("nav-item");
                for (var i = 0; i < navItems.length - 1; i++) {
                    if (window.location.href.includes(navItems[i].firstElementChild.getAttribute("href"))) {
                        navItems[i].className += " active";
                        break;
                    }
                }
            });

        </script>
    </div>
</body>
</html>

@model IPagedList<LendCar.Models.Vehicle>

        @for (int i = 0; i < Model.Count; i++)
        {
            <div class="col-md-10">
                <div class="posts-thumb pull-left">
                    <a href="#"><img alt="img" src="@Url.Content(Model[i].ImageUrl)"></a>
                </div>
                <div class="post-info">
                    <h4 class="entry-title">
                        <a href="#">@Model[i].Id</a>
                    </h4>
                    <p>4.2 <i class="fas fa-star"></i></p>
                    <span>(116 Trip)</span>
                    <p><i class="fa fa-car"></i> Gas</p>
                    <p><i class="fa fa-group "></i> 5 seats</p>
                </div>
            </div>
            <div class=" col-md-2">
                <a class="fas fa-trash-alt carList" data-id="@Model[i].Id"> Delete</a>
            </div>
        }